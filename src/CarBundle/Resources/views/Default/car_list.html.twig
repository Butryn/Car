{% extends 'CarBundle::layout.html.twig' %}
{% block content %}
<table>
	{% for car in cars %}
	<tr>
		<td><img src="{{car.img}}"</td>
		<td>{{car.brand}}</td>
		<td>{{car.model}}</td>
		<td>{{car.power}}</td>
		<td>{{car.type}}</td>
		<td>{{car.price}}</td>
		<td><a class="btn btn-primary" href="{% if app.user %}{{'order'}}/{{car.id}}{% else %}{{path('fos_user_security_login')}}{% endif %}">Wypo≈ºycz</a></td>
	</tr>
	{% endfor %}
</table>
{% endblock %}